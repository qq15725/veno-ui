{
  "name": "veno-ui",
  "version": "0.4.0",
  "description": "A Vue 3 UI Library. Uses Composable. Uses TypeScript.",
  "homepage": "https://www.venojs.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qq15725/veno-ui.git"
  },
  "keywords": [
    "veno-ui",
    "ui library",
    "vue"
  ],
  "main": "lib/framework.js",
  "module": "lib/framework.js",
  "types": "lib/framework.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./lib/framework.js",
      "require": "./dist/veno-ui.js"
    },
    "./styles": "./lib/styles/main.scss",
    "./styles/*": "./lib/styles/*",
    "./framework": "./lib/framework.js",
    "./components": "./lib/components/index.js",
    "./components/*": "./lib/components/*/*.js",
    "./directives": "./lib/directives/index.js",
    "./directives/*": "./lib/directives/*/*.js",
    "./providers": "./lib/providers/index.js",
    "./iconsets/*": "./lib/iconsets/*.js",
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "lib/framework.js": [
        "lib/framework.d.ts"
      ],
      "framework": [
        "lib/framework.d.ts"
      ],
      "*": [
        "*",
        "dist/*",
        "lib/*",
        "lib/*.d.ts",
        "lib/*/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build:dist": "vite build -c vite.dist.config.ts && vite build -c vite.dist-min.config.ts",
    "build:lib": "vite build -c vite.lib.config.ts",
    "build": "npm run build:lib && npm run build:dist"
  },
  "devDependencies": {
    "@vue/runtime-core": "^3.2.31",
    "@vue/runtime-dom": "^3.2.31",
    "@vueuse/head": "^0.7.5",
    "vite-plugin-dts": "^0.9.6",
    "vue-router": "^4.0.12"
  },
  "peerDependencies": {
    "prismjs": "^1.25.0",
    "vue": "^3.2.31"
  },
  "peerDependenciesMeta": {
    "prismjs": {
      "optional": true
    }
  },
  "sideEffects": [
    "*.sass",
    "*.scss",
    "*.css",
    "*.vue"
  ],
  "publishConfig": {
    "access": "public"
  }
}
