{
  "name": "veno-ui",
  "version": "0.4.24",
  "description": "A Vue 3 UI Library. Uses Composable.",
  "author": "wxm",
  "license": "MIT",
  "keywords": [
    "veno-ui",
    "ui library",
    "vue"
  ],
  "homepage": "https://github.com/qq15725/veno-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qq15725/veno-ui.git"
  },
  "bugs": {
    "url": "https://github.com/qq15725/veno-ui/issues"
  },
  "scripts": {
    "typecheck": "tsc --noEmit --skipLibCheck --project ./tsconfig.json",
    "build": "pnpm run build:dts && pnpm run build:dist && pnpm run build:lib",
    "build:dts": "tsc --pretty --emitDeclarationOnly -p ./tsconfig.json",
    "build:dist": "rollup -c rollup.dist.config.js",
    "build:lib": "rollup -c rollup.lib.config.js",
    "clean": "rimraf lib dist"
  },
  "files": [
    "lib",
    "dist"
  ],
  "sideEffects": [
    "*.sass",
    "*.scss",
    "*.css"
  ],
  "exports": {
    ".": {
      "import": "./lib/framework.mjs",
      "require": "./dist/veno-ui.mjs"
    },
    "./components": "./lib/components/index.mjs",
    "./components/*": "./lib/components/*/*.mjs",
    "./directives": "./lib/directives/index.mjs",
    "./directives/*": "./lib/directives/*/*.mjs",
    "./framework": "./lib/framework.mjs",
    "./iconsets/*": "./lib/iconsets/*.mjs",
    "./providers": "./lib/providers/index.mjs",
    "./styles": "./lib/styles/main.scss",
    "./styles/*": "./lib/styles/*",
    "./utils": "./lib/utils/index.mjs",
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "lib/framework.mjs": [
        "lib/framework.d.ts"
      ],
      "framework": [
        "lib/framework.d.ts"
      ],
      "*": [
        "*",
        "dist/*",
        "lib/*",
        "lib/*.d.ts",
        "lib/*/index.d.ts"
      ]
    }
  },
  "main": "lib/framework.mjs",
  "module": "lib/framework.mjs",
  "types": "lib/framework.d.ts",
  "peerDependencies": {
    "vue": "^3.2.4"
  },
  "dependencies": {
    "@veno-ui/utils": "workspace:*"
  },
  "devDependencies": {
    "@vueuse/head": "^1.1.23",
    "magic-string": "^0.30.0",
    "vue-router": "^4.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
