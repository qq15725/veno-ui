{
  "name": "veno-ui",
  "version": "0.0.6",
  "description": "A Vue.js 3 UI Library",
  "homepage": "http://venoui.fdota.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qq15725/veno-ui.git"
  },
  "keywords": [
    "veno-ui",
    "ui library",
    "vue"
  ],
  "main": "lib/framework.js",
  "module": "lib/framework.js",
  "types": "lib/framework.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./lib/framework.js",
      "require": "./dist/veno-ui.js"
    },
    "./styles": "./lib/styles/main.scss",
    "./styles/*": "./lib/styles/*",
    "./framework": "./lib/framework.js",
    "./components": "./lib/components/index.js",
    "./directives": "./lib/directives/index.js",
    "./directives/*": "./lib/directives/*/*.js",
    "./iconsets/*": "./lib/iconsets/*.js",
    "./*": "./*"
  },
  "scripts": {
    "build:dist": "vite build -c vite.dist.config.ts",
    "build:lib": "vite build -c vite.lib.config.ts",
    "build": "npm run build:lib && npm run build:dist"
  },
  "devDependencies": {
    "@types/node": "^14.11.10",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.2",
    "vite": "^2.6.14",
    "vite-plugin-dts": "^0.9.6",
    "vue-router": "^4.0.12"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "sideEffects": [
    "*.sass",
    "*.scss",
    "*.css",
    "*.vue"
  ],
  "publishConfig": {
    "access": "public"
  }
}
