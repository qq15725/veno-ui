{
  "compilerOptions": {
    "outDir": "es",
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "removeComments": true,
    "strict": true,
    "allowJs": true,
    // "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "types": [
      "node",
      "vue-router"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}