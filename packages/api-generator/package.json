{
  "name": "@veno-ui/api-generator",
  "version": "0.5.1",
  "description": "Api generator for Veno UI",
  "author": "wxm",
  "license": "MIT",
  "keywords": [],
  "homepage": "https://github.com/qq15725/veno-ui/blob/master/packages/api-generator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qq15725/veno-ui.git"
  },
  "bugs": {
    "url": "https://github.com/qq15725/veno-ui/issues"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf dist"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "ts-morph": "^14.0.0",
    "vue": "^3.2.47"
  },
  "dependencies": {
    "@veno-ui/utils": "workspace:*",
    "veno-ui": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
