{
  "name": "@veno-ui/vite-plugin-markdown",
  "version": "0.5.4",
  "description": "Vite Markdown Plugin for VenoUI",
  "author": "wxm",
  "license": "MIT",
  "keywords": [
    "vite-plugin",
    "vite-plugin-markdown",
    "veno-ui md"
  ],
  "homepage": "https://github.com/qq15725/veno-ui/blob/master/packages/vite-plugin-markdown",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qq15725/veno-ui.git"
  },
  "bugs": {
    "url": "https://github.com/qq15725/veno-ui/issues"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf dist"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "peerDependencies": {
    "vite": "^2.5.0 || ^3.0.0-0 || ^4.0.0"
  },
  "dependencies": {
    "@veno-ui/markdown": "workspace:*",
    "@veno-ui/utils": "workspace:*",
    "local-pkg": "^0.4.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
