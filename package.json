{
  "name": "@veno-ui/monorepo",
  "version": "0.4.24",
  "private": true,
  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "dev": "pnpm --filter docs dev",
    "build": "pnpm --filter !docs run clean && pnpm --filter !docs run build",
    "build:packages": "pnpm --filter @veno-ui/* run clean && pnpm --filter @veno-ui/* run build",
    "build:docs": "pnpm run build && pnpm --filter docs run build",
    "lint": "eslint \"packages/*/{src,scripts}/**/*.{ts,tsx,js}\"",
    "lint:fix": "eslint \"packages/*/{src,scripts}/**/*.{ts,tsx,js}\" --fix",
    "publish": "pnpm -r publish",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "release": "bumpp package.json packages/*/package.json --commit \"chore(release): publish v%s\" --push --tag"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-syntax-typescript": "^7.21.4",
    "@qq15725/eslint-config": "^1.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/babel__core": "^7.20.0",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "autoprefixer": "^10.4.14",
    "bumpp": "^9.1.0",
    "conventional-changelog-cli": "^2.2.2",
    "convert-source-map": "^2.0.0",
    "cssnano": "^6.0.0",
    "esbuild": "^0.17.17",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.38.0",
    "gh-pages": "^5.0.0",
    "glob": "^10.1.0",
    "mkdirp": "^3.0.0",
    "postcss": "^8.4.22",
    "rimraf": "^5.0.0",
    "rollup": "^3.20.6",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-pure": "^0.0.2",
    "rollup-plugin-sass": "^1.12.19",
    "sass": "^1.62.0",
    "ts-morph": "^18.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.1",
    "vue": "^3.2.47"
  }
}
