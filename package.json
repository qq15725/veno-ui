{
  "private": true,
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "markdown:dev": "npm run dev -w @veno-ui/markdown",
    "docs-plugin:dev": "npm run dev -w @veno-ui/vite-plugin-vue-docs",
    "docs:dev": "npm run dev -w venojs.com",
    "docs:build": "npm run build -w venojs.com",
    "docs:publish": "NODE_DEBUG=gh-pages gh-pages -d packages/docs/dist",
    "docs:update": "npm run docs:build && npm run docs:publish"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@types/node": "^14.11.10",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "gh-pages": "^3.2.3",
    "lerna": "^3.20.2",
    "sass": "^1.39.0",
    "sass-loader": "^10.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.4",
    "vue": "^3.2.26"
  }
}
