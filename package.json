{
  "name": "@veno-ui/monorepo",
  "version": "0.4.17",
  "private": true,
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "dev": "pnpm run dev --filter @veno-ui/docs",
    "docs:build": "pnpm run build-post --filter @veno-ui/docs",
    "docs:publish": "NODE_DEBUG=gh-pages gh-pages -d packages/docs/dist",
    "docs:update": "pnpm run docs:build && pnpm run docs:publish",
    "build": "pnpm -r run build",
    "release": "bumpp package.json packages/*/package.json --commit \"chore(release): publish v%s\" --push --tag",
    "publish": "pnpm -r publish"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "bumpp": "^7.1.1",
    "gh-pages": "^3.2.3",
    "sass": "^1.50.0",
    "ts-morph": "^14.0.0",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.2",
    "vite": "^2.9.1",
    "vue": "^3.2.32"
  }
}
